<UserControl x:Class="AirSimApp.Controls.Altimeter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AirSimApp.Controls"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="50">
    <Grid>
        <StackPanel Orientation="Horizontal"
                    HorizontalAlignment="Right">
            <local:ClickedOdometerDigit x:Name="Digit4" />
            <local:ClickedOdometerDigit x:Name="Digit3" />
            <local:ClickedOdometerDigit x:Name="Digit2" />
            <local:OdometerDigit x:Name="Digit1" />
            <local:OdometerDigit x:Name="Digit0" />
        </StackPanel>
    </Grid>
</UserControl>